{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/counsellors",
      "rewrite": "/index.html"
    },
    {
      "route": "/counsellors/:speciality",
      "rewrite": "/index.html"
    },
    {
      "route": "/about",
      "rewrite": "/index.html"
    },
    {
      "route": "/contact",
      "rewrite": "/index.html"
    },
    {
      "route": "/my-profile",
      "rewrite": "/index.html"
    },
    {
      "route": "/my-appointments",
      "rewrite": "/index.html"
    },
    {
      "route": "/appointment/:docId",
      "rewrite": "/index.html"
    },
    {
      "route": "/login",
      "rewrite": "/index.html"
    },
    {
      "route": "/forgot-password",
      "rewrite": "/index.html"
    },
    {
      "route": "/password-reset/:id/:token",
      "rewrite": "/index.html"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  }
}
